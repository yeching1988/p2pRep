ul {
  margin: 0;
  padding: 0;
  list-style: none;
}
body {
  padding: 0;
  margin: 0;
}
.nav .tooltip img {
  width: 120px;
}
.navbar-inverse .navbar-brand {
  color: #fff;
  font-size: 26px;
  padding: 10px 15px;
}
.navbar-inverse .navbar-nav > li > a {
  color: #fff;
}
.navbar-inverse .navbar-nav > li > a:hover {
  color: #3aabf7;
  text-decoration: underline;
}
.navbar:nth-of-type(1) {
  border-radius: 0;
  margin-bottom: 0;
}
.navbar:nth-of-type(2) {
  margin-bottom: 5px;
}
.navbar-default .navbar-nav li a {
  line-height: 46px;
  font-size: 16px;
  text-align: center;
}
.navbar-default .navbar-nav > li > a:hover {
  color: #2e8ece;
  background-color: #e7e7e7;
}
body {
  padding: 0;
  margin: 0;
}
#leftSide .panel .panel-title {
  text-indent: 30px;
}
#leftSide .panel .panel-body {
  padding: 0;
}
#leftSide .panel .panel-body ul li {
  line-height: 38px;
  text-indent: 55px;
}
#leftSide .panel .panel-body ul li.active {
  background-color: #337ab7;
}
#leftSide .panel .panel-body ul li.active a {
  color: #fff;
}
#rightSide #toggleBtn {
  display: none;
}
#rightSide .panel-body .rightCash {
  margin: 20px auto;
}
#rightSide .panel-body #AuthIcon {
  margin-top: 20px;
}
#rightSide .panel-body #AuthIcon .auth {
  padding-top: 15px;
}
#rightSide .panel-body #AuthIcon .auth img {
  margin-right: 20px;
}
#rightSide .panel-body #AuthIcon .auth p {
  margin-top: 0;
  font-weight: bold;
  font-size: 14px;
}
#mainContent {
  position: relative;
  height: 920px;
}
@media all and (max-width: 320px) {
  #mainContent {
    height: 950px;
  }
}
@media all and (max-width: 768px) {
  body {
    overflow-x: hidden;
  }
  #leftSide {
    position: absolute;
    left: 0;
  }
  #rightSide {
    position: absolute;
    right: -50%;
    transition: all 1s ease-in;
  }
  #rightSide #toggleBtn {
    width: 20px;
    height: 20px;
    background: #337ab7;
    line-height: 20px;
    text-align: center;
    color: #fff;
    padding: 0 5px;
    border-radius: 5px;
    display: block;
    position: absolute;
    top: 326px;
  }
  #rightSide.active {
    right: 0;
    transition: all 1s ease-in;
  }
}
footer {
  background-color: #F0F0F0;
  padding: 30px 0;
  margin-top: 20px;
}
footer .container p a {
  color: #2e8ece;
}

